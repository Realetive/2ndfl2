mixin navbar(data)
  include ../menu/menu

  .navbar
    a.navbar__logo(href='/')
      != jadeHelpers.Icon.call(locals, {iconName: 'logo', className: 'icon icon_logo'})
    .navbar__menu
      +menu('menu')
    a.navbar__contacts(href='tel:+79216556291')
      != jadeHelpers.Icon.call(locals, {iconName: 'call', className: 'icon icon_call'})